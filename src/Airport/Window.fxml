<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Airport.Controller">
   <children>
      <HBox prefHeight="30.0" prefWidth="200.0">
         <children>
            <Label prefHeight="20.0" prefWidth="150.0" text="Choose airport:">
               <HBox.margin>
                  <Insets left="90.0" right="20.0" top="3.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfAirport" prefHeight="25.0" prefWidth="170.0" />
         </children>
      </HBox>
      <Button fx:id="btChoose" mnemonicParsing="false" onAction="#onActionBTChoose" prefHeight="25.0" prefWidth="200.0" text="Choose">
         <VBox.margin>
            <Insets bottom="15.0" left="180.0" top="10.0" />
         </VBox.margin>
      </Button>
      <TableView fx:id="tvTable" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="200.0" prefWidth="560.0">
        <columns>
          <TableColumn maxWidth="0.0" minWidth="0.0" prefWidth="0.0" />
        </columns>
      </TableView>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="30.0" prefWidth="560.0">
         <children>
            <Label prefHeight="20.0" prefWidth="145.0" text="Number of flight:">
               <HBox.margin>
                  <Insets bottom="10.0" left="15.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfIdFlight" prefHeight="25.0" prefWidth="200.0">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="30.0" prefWidth="560.0">
         <children>
            <Label prefHeight="20.0" prefWidth="170.0" text="From:">
               <HBox.margin>
                  <Insets left="15.0" right="10.0" />
               </HBox.margin>
            </Label>
            <Label prefHeight="20.0" prefWidth="170.0" text="To:">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="btFind" mnemonicParsing="false" onAction="#onActionBTFind" prefHeight="25.0" prefWidth="160.0" text="Find">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="50.0" prefWidth="200.0">
         <children>
            <TextField fx:id="tfFrom" prefHeight="25.0" prefWidth="170.0">
               <HBox.margin>
                  <Insets left="15.0" right="8.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="tfTo" prefHeight="25.0" prefWidth="170.0">
               <HBox.margin>
                  <Insets left="8.0" right="8.0" />
               </HBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextField>
         </children>
      </HBox>
      <Label fx:id="lblNotification" alignment="CENTER" prefHeight="20.0" prefWidth="560.0">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin></Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
